<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="TestErrorApplyQueryAttributes.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:views="clr-namespace:TestErrorApplyQueryAttributes.MVVM.Views"
    xmlns:local="clr-namespace:TestErrorApplyQueryAttributes"
    Shell.FlyoutBehavior="Disabled">

    <TabBar>
        <Tab Title="Search Beers"
             Icon="search">
            <ShellContent Title="Page1"
